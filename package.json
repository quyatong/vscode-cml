{
	"name": "cml",
	"description": "Syntax Highlight for Chameleon",
	"version": "0.0.1",
	"author": "quyatong",
	"publisher": "quyatong",
	"icon": "images/logo.png",
	"keywords": [
		"cml",
		"chameleon"
	],
	"engines": {
		"vscode": "^0.10.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/quyatong/vscode-cml.git"
	},
	"categories": [
		"Languages"
	],
